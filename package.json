{
  "name": "@lealt/rias",
  "version": "1.1.0",
  "description": "A modern, developer-friendly Lavalink v4 client for Discord.js with excellent TypeScript support",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx watch src/index.ts",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build",
    "prepare": "husky"
  },
  "keywords": [
    "lavalink",
    "discord",
    "discord.js",
    "music",
    "audio",
    "typescript",
    "lavalink-v4",
    "music-bot"
  ],
  "author": "Lealt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lealtt/rias.git"
  },
  "bugs": {
    "url": "https://github.com/lealtt/rias/issues"
  },
  "homepage": "https://github.com/lealtt/rias#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "discord.js": "^14.0.0"
  },
  "dependencies": {
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.1",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^4.0.2",
    "discord.js": "^14.23.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ]
}
