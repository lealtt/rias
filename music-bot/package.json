{
  "name": "rias-music-bot",
  "version": "1.0.0",
  "description": "A sample Discord music bot using the Rias library",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "tsx --env-file .env src/index.ts",
    "build": "tsc",
    "start": "node --env-file .env .",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "lavalink",
    "rias"
  ],
  "license": "MIT",
  "dependencies": {
    "@lealt/rias": "file:..",
    "discord.js": "^14.23.2"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
